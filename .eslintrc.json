{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "eslint-config-prettier",
        "prettier",
        "prettier/react"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "comma-dangle": "off",
        "react/jsx-filename-extension": "off"
    }
}
